    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-share": "^4.4.0",
    "react-toggle-component": "^3.0.8",
    "react-twitter-widgets": "^1.10.0",
    "react-typography": "^0.16.20",
    "sass": "^1.49.7",
